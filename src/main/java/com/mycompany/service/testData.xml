<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
"http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
		<bean id="contact1" class="com.mycompany.Contact">
			<property name="contactId">
				<value>1</value>
			</property>
			<property name="email">
				<value>contact1@hotmail.com</value>
			</property>	
			<property name="firstName">
				<value>contact1</value>
			</property>
			<property name="lastName">
				<value>contact1last</value>
			</property>
		</bean>
		<bean id="contact2" class="com.mycompany.Contact">
			<property name="contactId">
				<value>2</value>
			</property>
			<property name="email">
				<value>contact2@hotmail.com</value>
			</property>	
			<property name="firstName">
				<value>contact2</value>
			</property>
			<property name="lastName">
				<value>contact2last</value>
			</property>
		</bean>
		<bean id="contact3" class="com.mycompany.Contact">
			<property name="contactId">
				<value>3</value>
			</property>
			<property name="email">
				<value>contact3@hotmail.com</value>
			</property>	
			<property name="firstName">
				<value>contact3</value>
			</property>
			<property name="lastName">
				<value>contact3last</value>
			</property>
		</bean>
</beans>
